<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">
        <record id="seq_patient_queue_token" model="ir.sequence">
            <field name="name">Patient Queue Token</field>
            <field name="code">patient.queue.token</field>
            <field name="prefix">TKN-</field>
            <field name="padding">4</field>
            <field name="number_increment">1</field>
            <field name="number_next_actual">1</field> <!-- reset starting number -->
            <field name="implementation">standard</field> <!-- critical for proper increment -->
            <field name="active">True</field>
        </record>
    </data>




</odoo>